{
  "canisters": {
    "main": {
      "candid": "backend/main/main.did",
      "package": "main",
      "type": "rust"
    },
    "platform": {
      "candid": "backend/platform/platform.did",
      "package": "platform",
      "type": "rust"
    },
    "user": {
      "candid": "backend/user/user.did",
      "package": "user",
      "type": "rust"
    },
    "platform_manager": {
      "candid": "backend/platform_manager/platform_manager.did",
      "package": "platform_manager",
      "type": "rust"
    },
    "user_manager": {
      "candid": "backend/user_manager/user_manager.did",
      "package": "user_manager",
      "type": "rust"
    },
    "frontend": {
      "dependencies": ["main"],
      "frontend": {
        "entrypoint": "src/index.html"
      },
      "source": [
        "src/assets",
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
